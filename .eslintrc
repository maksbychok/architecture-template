{
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "import/no-unresolved": [2, { "caseSensitive": false }],
    "no-underscore-dangle": [2, { "allowAfterThis": true }],
    "class-methods-use-this": [0],
    "no-new": [0],
    "no-unused-vars": [0],
    "no-use-before-define": [0],
    "no-plusplus": [0],
    "consistent-return": [0]
  }
}
